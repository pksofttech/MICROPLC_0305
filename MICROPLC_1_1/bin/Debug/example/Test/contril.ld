# LD FORMAT copy Form LDMICRO#
HARDWARE
Micro PLC deca
IO LIST
X1 at X02
Yout at Y00
END
PROGRAM
RUNG
	PARALLEL
		SERIES
			PARALLEL
				SERIES
					CONTACTS X1 1 0 0
					CONTACTS R2 0 0 0
				END
				SERIES
					CONTACTS X1 0 0 0
					CONTACTS R1 0 0 0
				END
			END
			COIL R1 0 0 0 0
		END
		SERIES
			PARALLEL
				SERIES
					CONTACTS X1 1 0 0
					CONTACTS R2 0 0 0
				END
				SERIES
					CONTACTS X1 0 0 0
					CONTACTS R1 1 0 0
				END
			END
			COIL R2 0 0 0 0
		END
	END
END
RUNG
	CONTACTS R2 0 0 0
	COIL Yout 0 0 0 0
END
