# LD FORMAT copy Form LDMICRO#
HARDWARE
ArUnitControl 32IO
IO LIST
XOL_m1 at OFF
Ydelta at OFF
Ymain at OFF
Ystra at OFF
END
PROGRAM
RUNG
	CONTACTS XOL_m1 1 0 0
	TPC T01 1000 1000
	TON Tm1 100000
	CTC Cm1 1
END
RUNG
	CONTACTS Cm1 0 1 0 1 0
	COIL Ymain 0 0 0 0
END
RUNG
	CONTACTS Ymain 0 0 0
	TOF Tdelta 1000
	CONTACTS Ymain 0 0 0
	COIL Ydelta 0 0 0 0
END
RUNG
	PARALLEL
		SERIES
			CONTACTS Ydelta 0 0 0
			TOF Tstra1 1000
			CONTACTS Ymain 0 0 0
		END
		CONTACTS Ystra 0 0 0
	END
	TON Tstra0 0000
	CONTACTS Ymain 0 0 0
	CONTACTS Ydelta 1 0 0
	COIL Ystra 0 0 0 0
END
