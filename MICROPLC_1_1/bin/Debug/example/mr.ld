# LD FORMAT copy Form LDMICRO#
HARDWARE
Arduino UNOR3
IO LIST
X1 at A0
X2 at A1
X3 at A2
X4 at A3
Y1 at D7
Y2 at D6
Ym1 at D13
Ym2 at D12
END
PROGRAM
RUNG
	CONTACTS X1 0 0 0
	PARALLEL
		MASTER_RELAY Master_Relay
		COIL Ym1 0 0 0 0
	END
END
RUNG
	CONTACTS X2 0 0 0
	COIL Y1 0 0 0 0
END
RUNG
	CONTACTS X3 0 0 0
	PARALLEL
		COIL Ym2 0 0 0 0
		MASTER_RELAY Master_Relay
	END
END
RUNG
	CONTACTS X4 0 0 0
	COIL Y2 0 0 0 0
END
