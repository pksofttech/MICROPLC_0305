# LD FORMAT copy Form LDMICRO#
HARDWARE
Arduino UNOR3
IO LIST
X1 at A0
X2 at A1
Y1 at D12
Y2 at D11
END
PROGRAM
RUNG
	CONTACTS Rosc 0 0 0
	SHIFT_REGISTER reg 7 
END
RUNG
	CONTACTS X1 0 0 0
	MOVE reg0 1
END
RUNG
	CONTACTS X2 0 0 0
	MOVE reg0 0
END
RUNG
	CONTACTS Rosc 1 0 0
	TOF Tof 500000
	TON Ton 500000
	COIL Rosc 0 0 0 0
END
RUNG
		COIL Y1 0 0 0 0
END
RUNG
		COIL Y2 0 0 0 0
END
